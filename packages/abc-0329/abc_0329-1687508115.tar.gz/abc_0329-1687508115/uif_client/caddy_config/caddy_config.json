{
  "admin": {
    "disabled": true
  },
  "logging": {
    "logs": {
      "default": {
        "level": "FATAL"
      }
    }
  },
  "apps": {
    "http": {
      "servers": {
        "not_safe": {
          "listen": [":80"],
          "routes": [{
            "match": [{
              {domain}
              "path": ["/vmess_ws"]
            }],
            "handle": [{
              "handler": "reverse_proxy",
              "transport": {
                "protocol": "http",
                "versions": ["2"]
              },
              "upstreams": [{
                "dial": "127.0.0.1:10111"
              }]
            }]
          }, {
            "match": [{
              {domain}
