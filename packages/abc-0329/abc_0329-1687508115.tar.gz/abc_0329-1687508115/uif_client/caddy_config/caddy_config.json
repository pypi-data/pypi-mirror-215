              {domain}
              "path": ["/vmess_grpc/*"]
            }],
            "handle": [{
              "handler": "reverse_proxy",
              "transport": {
                "protocol": "http",
                "versions": ["h2c", "2", "1.1"]
              },
              "upstreams": [{
