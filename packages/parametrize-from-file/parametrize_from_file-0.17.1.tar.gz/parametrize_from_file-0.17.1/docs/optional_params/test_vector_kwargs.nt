test_from_angle:
  -
    angle: 0
    expected: Vector(1, 0)
  -
    angle: pi / 4
    expected: Vector(1 / sqrt(2), 1 / sqrt(2))
  -
    angle: pi / 2
    expected: Vector(0, 1)
  -
    angle: 90
    kwargs:
      unit: 'deg'
    expected: Vector(0, 1)
  -
    angle: 0
    kwargs:
      magnitude: 2
    expected: Vector(2, 0)
